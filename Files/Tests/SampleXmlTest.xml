  <test name="CME_Decimals">
    <desc>Decimals Test from CME</desc>
    <td:templates>
      <template name="TestTemplate" id="1" xmlns="http://www.fixprotocol.org/ns/fast/td/1.1">
        <typeRef name="TestTemplate"/>
        <decimal id="1" name="Field1"/>
        <decimal id="2" presence="optional" name="Field2"/>
        <decimal id="3" presence="optional" name="Field3">
          <exponent>
            <copy/>
          </exponent>
          <mantissa>
            <copy/>
          </mantissa>
        </decimal>
        <decimal id="4" presence="optional" name="Field4">
          <exponent>
            <increment/>
          </exponent>
          <mantissa>
            <increment/>
          </mantissa>
        </decimal>
        <decimal id="5" name="Field5">
          <exponent>
            <delta/>
          </exponent>
          <mantissa>
            <delta/>
          </mantissa>
        </decimal>
        <decimal id="6" presence="optional" name="Field6">
          <exponent>
            <constant value="1"/>
          </exponent>
          <mantissa>
            <constant value="520"/>
          </mantissa>
        </decimal>
      </template>
    </td:templates>
    <binstr>
      11111110 10000001 11111110 00001000 10011111 11111111 00000001 11011110 11111111 00010010 10101000 11111111 00000010 11010110 11111111 00011010 11111100 10011110 11111110 00001000 10011110 11111111 00000001 11101000 00000010 11001110 11111111 00000001 11110010 10000000 01100111 11011010 10111110 11111101 00000000 01010010 10100110 11111111 10100000 11111110 00011010 10010001 11111101 00000001 00111111 10000100 10000000 00011001 10000000 10111110 11111110 01111000 10000001 11111111 01101101 11010000 11111111 01100100 10110010 11111111 11010110 10000000 01000111 10001100
    </binstr>
    <data>
      <message>
        TestTemplate -> {1, 10.55, 22.2, 234.4, 34.2, 345.2, 5200, }
      </message>
      <message>
        TestTemplate -> {1, 10.54, 23.2, 33.4, 24.2, 34.2, 5200, }
      </message>
      <message>
        TestTemplate -> {1, 10.534, 3.2, 33.45, 24.452, 354.2, 5200, }
      </message>
      <message>
        TestTemplate -> {1, -10.23, -235.2, -353.4, -4.2, -374.2, 5200, }
      </message>
    </data>
  </test>

